a, address, b, big, blockquote, body, button, center, cite, code, dd, del, div, dl, dt, em, fieldset, form, h1, h2, h3, h4, h5, h6, html, i, iframe, img, input, ins, label, legend, li, ol, option, p, pre, select, small, span, strong, textarea, u, ul {
    margin: 0;
    padding: 0;
}
html, body{
    display: block;
    font-size: 14px;
    font-family: "微软雅黑";
}
ul,li{
    list-style: none;
}
a{
    text-decoration: none;
}
.container{
    width: 1200px;
    margin: 0 auto;
}
.clearfix::after{
    content: "";
    height: 0;
    display: block;
    clear: both;
    visibility: hidden;
}
