/* 楼层导航开始 */
.main .floor{
    position: fixed;
    top: 50%;
    right: 30px;
    margin-top: -145px;
    width: 205px;  
    transition: all .5s;
    transform-origin: 100% top;
}
.floor-change{
    transform:scaleY(0); 
}
.floor-back{
    transform:scaleY(1);
}
.main .floor .top-img h1{
    margin-bottom: -25px;
    margin-left: 10px;
    width: 200px;
    height: 115px;
    background: url(../images/Section5/fool.png) no-repeat 0 0;
}
.main .floor .floor-box{
    width: 117px;
    margin: 0 auto;
}
.main .floor .floor-box li{
    width: 117px;
    height: 37px;
    text-align: center;
    line-height: 37px;
    cursor: pointer;
    background-color: #8700D5;
    color: #fff;
}
.main .floor .floor-box li:hover{
    background-color: #BD4CFF;
}
.floor-hover{
    background-color: #BD4CFF!important;
}
.main .floor .retrun-top{
    margin: 0 auto;
    width: 117px;
    height: 37px;
    text-align: center;
    line-height: 37px;
    cursor: pointer;
    background-color: #8700D5;  
    color: #fff;
}
.main .floor .bottom-img{
    margin: 0 auto;
    width: 117px;
    height: 14px;
    background: url(../images/Section5/floatmenu-bottom.png) no-repeat 0 0 ;
}