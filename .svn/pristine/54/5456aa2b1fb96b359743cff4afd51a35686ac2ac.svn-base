/* 主体开始 */
/* banner轮播图开始 */
.main .banner-loop{
    position: relative;
    min-width: 1200px;
    height: 450px;
    /* border: 1px solid red; */
    margin: 0 auto;
    /* overflow: hidden; */
}
.main .banner-loop .loop a{
    height: 100%;
    float: left;
}
.main .banner-loop .loop li{
    width: 100%;
    height: 450px;
    position: absolute;
    left: 0;
    top: 0; 
    display: none;
}
.main .banner-loop .banner-next{
    position: relative;
    top: 420px;
    width: 1200px;
    height: 8px;
    margin: 0 auto;
}
.main .banner-loop .banner-next ul li {
    width: 50px;
    height: 8px;
    float: left;
    margin-right: 15px;
    cursor: pointer;
    background-color: #fff;
}
.main .banner-loop .banner-next ul{
    position: absolute;
    bottom: 0;
    left: 50%;
    margin-left: -95px;
}
/* content开始 */
.content-first{
    background-color: #FF3062;
}
.content-secend{
    background-color: #FFB124;
}
.content-third{
    background-color: #11E3D2;
}
.content-head-first{
    background: url(../images/Section5/list-head1.png) no-repeat 0 0 ;
}
.content-head-secend{
    background: url(../images/Section5/list-head2.png) no-repeat 0 0 ;
}
.content-head-third{
    background: url(../images/Section5/list-head3.png) no-repeat 0 0 ;
}
.main .content .content-head{
    width: 1200px;
    margin: 0 auto;
    overflow: hidden;
}
.main .content .content-head div{
    width: 482px;
    height: 118px;
    margin:  55px 359px;
}
.main .content .section1{
    width: 1200px;
    margin: 0 auto 30px;
    position: relative;
}
.main .content .section1 p{
    width: 1200px;
    height: 825px;
    margin: 0 auto;
}
.main .content .section1  .asidetop{
    position: absolute;
    right: 40px;
    top: 10px;
    width: 140px;
    height: 120px;
}
.main .content .section1  .asidetop .tehui{
    width: 140px;
    height: 45px;
    background-color: #CC00CC;
    color: #fff;
    text-align: center;
    line-height: 45px;
    font-size: 30px;
}
.main .content .section1  .asidetop .zhengtao{
    width: 140px;
    height: 25px;
    text-align: center;
    background-color: #FFFF00;
    color: red;
    line-height: 25px;
    font-size: 2px;

}
.main .content .section1  .asidetop .lijian{
    color: red;
    height: 50px;
    width: 140px;
    background-color: #FFFF00;
    text-align: center;
    line-height: 50px;
    font-size: 30px;
}
.main .content .section1 .asidebottom{
    position: absolute;
    bottom: 30px;
    right: 40px;
}
.main .content .section1 .asidebottom .price{
    color: #fff;
    font-size: 55px;
}
.main .content .section1 .asidebottom .a-suit-btn{
    width: 220px;
    height: 60px;
    border-radius: 50px;
    text-align: center;
    line-height: 60px;
    background-color: #FFFF00;
    font-size: 25px;
}
.main .content .tagbox{
    width: 1200px;
    margin: 0 auto;
}
.main .content .tagbox ul li{
    width: 380px;
    height: 195px;
   float: left;
   
}
.ml{
    margin-right:30px;   
}
.main .content .tagbox .title{
    width: 350px;
    height: 42px;
    border-radius: 10px 10px 0  0 ;
    padding-left: 30px;
    font-size: 25px;
    font-weight: bold;
    line-height: 42px;
    color: #fff;
}
/* 复用开始 */
.bc1{
    background-color: #9DD600;
}
.bc2{
    background-color: #DF7EFF;
}
.bc3{
    background-color: #7EC0FF;
}
.border1{
    border:1px solid #9DD600;
    border-radius: 20px;
}
.border2{
    border:1px solid #DF7EFF;
    border-radius: 20px;
}
.border3{
    border:1px solid #7EC0FF;
    border-radius: 20px;
}
.border1:hover{
    background-color: #9DD600;
}
.border2:hover{
    background-color: #DF7EFF;
}
.border3:hover{
    background-color: #7EC0FF;
}
/* 复用结束 */
.main .content .tagbox .taglist{
    width: 320px;
    height: 85px;
    padding: 30px; 
    background-color: #fff;
    border-radius: 0  0  10px 10px; 
}
.main .content .tagbox .taglist{
    width: 340px;
    height:85px;
    padding: 30px 20px;  
}
.main .content .tagbox .taglist a{
     float: left;
     width: 78px;
     height: 30px;
     text-align: center;
     line-height: 30px;
     color: #666666;
     margin-right: 5px;
     margin-bottom: 10px;
}
.main .content .tagbox .taglist a:hover{
     color: #fff;
}
.main .content .tagbox ul{
    margin-bottom: 20px;
}
.main .content .tab-box{
    width: 1200px;
    margin: 0 auto;
    padding-bottom: 30px;
}
.main .content .tab-box .tab-hd span{
    float: left;
    width: 240px;
    height: 50px;
    font-size: 28px;
    font-weight: bold;
    color: #fff;
    text-align: center;
    line-height: 50px;
    border-radius: 10px 10px 0 0;
    background-color: #633EF1;
    cursor: pointer;
}
.main .content .tab-box .tab-hd span:nth-child(2n){
    background-color: #7EC0FF;
}
.bchange{
    background-color: #fff!important;
    color: #633EF1!important;;
}
.main .content .tab-box .tab-list{
    margin: 0 auto; 
    margin-top: 5px;
    position: relative;
    width: 1200px;
    height: 310px;
    overflow: hidden;
}
.main .content .tab-box .tab-list .tab-u{
    position: absolute;
    left: 0;
    top: 0;
    
}
.main .content .tab-box .tab-list .tab-u li{
    float: left;
    margin-right: 4px;
    width: 196px;
    height: 310px;
    background-color: #fff;
}
.main .content .tab-box .tab-list .tab-u .tab-img{
    width: 196px;
    height: 200px;
    text-align: center;
}
.main .content .tab-box .tab-list .tab-u .tab-img img{
    width: 160px;
    height: 160px;
    margin-top: 20px;
    transition: all .5s;
}
.main .content .tab-box .tab-list .tab-u .tab-img img:hover{
    margin-left: 20px;
}
.main .content .tab-box .tab-list .tab-u .tab-name{
    font-size: 14px;
    padding: 0 18px;
    padding-top: 0px;
    padding-right: 18px;
    padding-bottom: 0px;
    padding-left: 18px;
    text-align: center;
    line-height: 23px;
    height: 46px;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    color: #666;
    margin-bottom: 10px;
}
.main .content .tab-box .tab-list .tab-u .tap-price{
    width: 170px;
    margin: 0 auto;

}
.main .content .tab-box .tab-list .tab-u .tap-price a{
    float: left;
    width: 170px;
    height: 35px;
    text-align: center;
    line-height: 35px;
    border-radius: 10px;
    color: #fff;
    font-size: 18px;
    background-color: #633EF1;
}
/* content结束 */
