/* content3开始 */
.main .content3{
    background-color: #DB004F;
}
.main .content3 .section4{
    padding: 30px;
    margin: 0 auto;
    width: 1200px;
} 
.main .content3 .section4 h1{
    text-align: center;
    font-size: 40px;
    color: #fff;
    margin-bottom: 20px;
}
