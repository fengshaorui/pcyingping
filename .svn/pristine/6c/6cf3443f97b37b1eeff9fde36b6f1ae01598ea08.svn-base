/*楼层导航样式*/
.lift{
    list-style: none;
    position: fixed;
    width: 40px;
    bottom: 50px;
    left: 50%;
    margin-left: -650px;
    transition:all .3s;
    transform-origin: 0 100%;
}
.hid{
    transform:scale(0);
}
.play{
    transform:scale(1);
}

.lift ul li{
    width: 24px;
    height: 30px;
    padding:5px 8px;
    text-align: center;
    background-color: #666666;
    margin-top:1px;
    font-size: 12px;
    color:white;
    /*line-height: 30px;*/
}
.lift ul li.scroll-color{
    background-color: #FF6600;
}
.lift ul li:hover{
    background-color: #FF6600;
}
.lift ul li{
    display: inline-block;;
    height: 30px;
    color:white;


}
.lift .first{
    display: inline-block;
    width: 40px;
    height: 40px;
    padding:0;
    font-size: 13px;
    line-height:40px;
    background-color: #FF6600;
    color:white;
    text-align: center;
}
.lift ul li.last{
    background-color: #8A8A8A
}





    
    
    

