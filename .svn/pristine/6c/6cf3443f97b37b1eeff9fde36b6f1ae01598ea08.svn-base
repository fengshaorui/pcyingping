/*公共样式*/
.bg{
    background-color: #F2F2F2; 
}
/*channel入口*/
.channel .channel-mid{
    width: 1200px;
    margin:0 auto;
    font-size: 0;
    padding-top:16px;
}
.channel .channel-mid ul.channel-item a{
    position: relative;
    display: inline-block;

}
.channel .channel-mid ul.channel-item li{
    overflow: hidden;
    float:left;
}
.channel .channel-mid ul.channel-item img{
    margin-left:10px;
    
}
.channel .channel-mid ul.channel-item a::after{
    transform: skew(20deg, 0deg);
    transition: all 0.5s ease 0s;
    position: absolute;
    /*top:-150px;*/
    left:270px;
    content: "";
    width: 70%;
    height: 150px;
    background-color: rgba(255,255,255,0.3);
    box-sizing: border-box;
}
.channel .channel-mid ul.channel-item a::before{
    transform: skew(20deg, 0deg);
    transition: all 0.5s ease 0s;
    position: absolute;
    /*top:-150px;*/
    left:-188px;
    content: "";
    width: 70%;
    height: 150px;
    background-color: rgba(255,255,255,0.3);
    box-sizing: border-box;
}
.channel .channel-mid ul.channel-item a.a-init::before{
    width: 73%;
}
.channel .channel-mid ul.channel-item li a:hover::before{
     left:-52px;
}
.channel .channel-mid ul.channel-item li a:hover::after{
    left:117px;
}
.channel .channel-mid ul.channel-item img.marg-init{
    margin-left:0;
}
