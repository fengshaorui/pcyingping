/* 主体内容 */
/* 广告 */
.container-z{
    width: 980px;
    margin: 0 auto;
}
.content .ad{
    width: 100%;
    height: 370px;
    background: url(../images/section1/ad1.jpg) 0 0;
}
/* 优惠券 */
.content .coupon{
    width: 100%;
    height: 216px;
    background: #febc27;
}
.content .coupon-box{ 
    height: 138px;
    margin: 0 auto;
    padding-top:20px; 
}
.content .coupon-img1{
    margin-left:60px; 
}
.content .coupon-bz{
    height: 38px;
    width: 570px;
    margin: 10px auto;
    line-height: 38px;
    color:red;   
}
/* 栏目 */
.content .article{
    height: 162px;
    background:#980035;
}
.content .article-box{
    margin: 0 auto;
    width: 981px;
    height: 129px;
    padding-top: 15px;
}
.content .article-img{
    margin-left:5px; 
}
/* 个人录音配音方案 */
.content .plate1-1{
    background: #000;
    height: 208px;
    width: 100%;
}
.content .plate1-1-bj{
    height: 208px;
    background: url(../images/section1/a_51.jpg);
} 
.content .plate1-2{
    height: 2813px;
    background:#510087;
}
/* 方案1 */
.content .plate1-2-1{
    height: 468px;
    /* border-bottom:1px solid #fff; */
    position: relative;
}
/* 套装立省 */
.content .plate1-lisheng-1{
    height: 35px;
    color:#fff;
    font-size:20px;
    font-weight:700;
    line-height: 35px;
    text-align: center;
    background: #cc00cc; 
}
.content .plate1-lisheng{
    background:#ffff00;
    width: 115px;
    height: 95px;
    position: absolute;
    top: 0;
    right: 0;
}
.content .plate1-lisheng-2{
    width: 115px;
}
.content .plate1-lisheng .lisheng1{
    color:#e5004f;
    text-align: center!important;
    font-size:14px;
    margin-top:3px; 
}
.content .plate1-lisheng .lisheng2{
    color:#e5004f;
    text-align: center;
    font-size: 35px;
    font-family: 'Arial';
    margin-top: -2px;
}
/* 套装立省结束 */
/* 立即购买 */
.content .plate1-buy{
    position: absolute;
    bottom: 10px;
    left: 20px;
}
.content .plate1-buy p{
    color: #ffff00;
    font-size: 20px;
    font-weight: 700;
}
.content .plate1-buy span{
    font-size: 55px;
    font-family: 'Arial';
    font-weight: 700;
}
.content .plate1-buy .plate1-buy-btn{
    background: #e5004f;
    width: 180px;
    height: 45px;   
    border-radius:35px;   
}
.content .plate1-buy .buy{
    color: #fff;
    font-size: 20px;
    font-family: '微软雅黑';
    margin-left:50px ;
    line-height: 45px;
}
/* 立即购买结束 */
/* 方案1结束 */

