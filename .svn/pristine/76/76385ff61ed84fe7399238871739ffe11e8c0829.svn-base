.top-ad{
    height: 100px;
    background: url("../images/detail/top-ad.jpg") no-repeat center center;
    background-color: #ce121a;
}


/* 网页header区域开始 */
.header{
    height: 36px;
    line-height: 36px;
    border-bottom: 1px solid #EDEEF0;
    background-color: #FCFCFC;
    font-family: "Microsoft YaHei", SimSun;
    font-size: 12px;
}
.header .container, .footer .container{
    width: 1210px;
}

/* 网页header左侧区域开始 */
.header .left{
    float: left;
    height: 100%;
}
.header .left span.login-info{
    color: #666;
}
.header .left span.login-tip{
    color: #ddd;
}
.header .left span.login-tip a{
    color: #ff6600;
}
.header .left span.login-tip a:hover{
    text-decoration: underline;
}
/* 网页header左侧区域结束 */

/* 网页header右侧区域开始 */
.header .right{
    float: right;
    height: 100%;
}
.header .right .list li{
    float: left;
    position: relative;
}
.header .right .list li:hover > a{
    color: #ff6600;
    border: 1px solid #eaeaea;
    border-bottom: none;
}
.header .right .list li  a{
    display: block;
    height: 14px;
    line-height: 16px;
    padding: 10px 20px;
    border: 1px solid #fcfcfc;
    color: black;
}
.header .right .list li  a i{
    font-size: 12px;
}
.header .right .list li .my-msg{
    position: absolute;
    left: 0;
    top: 36px;
    width: 122px;
    background-color: white;
    display: none;
}
.header .right .list li .my-msg a{
    border: 1px solid #eaeaea;
    border-bottom: none;
}
.header .right .list li .my-msg a:hover{
    color: #ff6600;
}
.header .right .my-yinping:hover .my-msg{
    display: block;
}
.header .right .contact-gif{
    vertical-align: middle;
}
.header .right .contact-box{
    position: absolute;
    right: 0;
    top: 36px;
    width: 250px;
    box-sizing: border-box;
    border: 1px solid #eaeaea;
    padding: 10px;
    background-color: white;
    display: none;
    z-index: 10;
}
.header .contact-box .top{
    position: relative;
    height: 130px;
    padding: 0 10px;
}
.header .contact-box .top img{
    width: 100px;
}
.header .right .contact-box .top a{
    position: absolute;
    right: 43px;
    top: 43px;
    background-color: #ff6600;
    font-size: 14px;
    height: 14px;
    border: none !important;
    border-radius: 30px;
    color: white !important;
    padding: 8px 10px !important;
    text-align: center;
    line-height: 14px;
}
.header .right .contact-box .top span{
    position: absolute;
    left: 50px;
    bottom: 18px;
    color: #666;
    font-size: 12px;
}
.header .right .contact-box .mid{
    height: 90px;
    box-sizing: border-box;
    padding: 0 10px;
    border-top: 1px solid #eaeaea;
    border-bottom: 1px solid #eaeaea;
}
.header .right .contact-box .mid-l{
    float: left;
    width: 60px;
    height: 60px;
    margin-top: 15px;
    background: url("../images/slideBar/phone.png") no-repeat center center;
}
.header .right .contact-box .mid-r{
    float: left;
    height: 60px;
    margin-top: 15px;
}
.header .right .contact-box .mid-r p.one{
    height: 30px;
    color: #ff6600;
    font-size: 20px;
    line-height: 30px;
}
.header .right .contact-box .mid-r p.two{
    height: 30px;
    color: #666;
    font-size: 12px;
    line-height: 30px;
}
.header .right .contact-box .bottom{
    height: 75px;
    padding: 10px;
}
.header .right .contact-box .bottom img.one{
    float: left;
    height: 60px;
    width: 60px;
    margin-top: 8px;
}
.header .right .contact-box .bottom p{
    float: left;
    color: #666;
    font-size: 12px;
    width: 50px;
    height: 30px;
    line-height: 15px;
    margin: 23px 3px 0 10px;
}
.header .right .contact-box .bottom img.two{
    float: left;
    height: 75px;
    width: 75px;
}
.header .right .contact:hover .contact-box{
    display: block;
}
/* 网页header右侧区域结束 */

/* 网站logo和全文搜索区域开始 */
.brand-box{
    height: 100px;
    margin-top: 20px;
}
.brand-box a.logo{
    float: left;
    width: 220px;
    height: 75px;
}
.brand-box a.hongbao{
    float: left;
    width: 180px;
    height: 70px;
    margin: 0 15px 0 10px;
}
.brand-box .brand-box-mid{
    float: left;
    width: 552px;
    height: 100px;
    /* background-color: blue; */
}
.brand-box .brand-box-mid .brand-mid-box{
    width: 100%;
    height: 60px;
    margin-top: 17px;
    /* background-color: red; */
}
.brand-box .brand-box-mid .brand-mid-box .search-box{
    height: 32px;
    /* box-sizing: border-box; */
    border: 2px solid #ff6600;
}
.brand-box .brand-box-mid .brand-mid-box .search-box input{
    float: left;
    width: 458px;
    height: 32px;
    font-size: 14px;
    box-sizing: border-box;
    padding-left: 15px;
    outline: none;
    border: none;
}
.brand-box .brand-box-mid .brand-mid-box .search-box a{
    float: left;
    width: 90px;
    height: 32px;
    background-color: #ff6600;
    line-height: 32px;
    text-align: center;
    font-size: 16px;
    color: white;
    cursor: pointer;
}
.brand-box .brand-box-mid .brand-mid-box .keywords a{
    float: left;
    font-size: 12px;
    margin-right: 10px;
    margin-top: 5px;
    color: #666;
}
.brand-box .brand-box-mid .brand-mid-box .keywords a:hover{
    color: #ff6600;
}

.brand-box .brand-box-right{
    float: right;
    width: 151px;
    height: 44px;
    margin-top: 17px;
}
.brand-box .brand-box-right a.one{
    position: relative;
    float: left;
    width: 60px;
    height: 40px;
    background: url("../images/detail/brand-right.jpg") no-repeat;
}
.brand-box .brand-box-right a.one::after{
    content: "";
    position: absolute;
    top: 3px;
    right: -6px;
    height: 34px;
    border-right: 1px solid #ddd;
}
.brand-box .brand-box-right a.two{
    float: right;
    width: 75px;
    height: 40px;
    background: url("../images/detail/brand-right.jpg") no-repeat;
    background-position-x: -59px; 
}

/* 网站logo和全文搜索区域结束 */

/* 网站导航区域开始 */
.navbar{
    height: 40px;
}
.navbar .item-box{
    position: relative;
    float: left;
    background-color: #ff6600;
    height: 40px;
    width: 210px;
    padding-left: 10px;
}
.navbar .item-box h3{
    height: 100%;
    line-height: 40px;
    color: white;
    font-size: 14px;
    letter-spacing: 1px;
    font-weight: normal;
}
/* 分类导航区域开始 */
.navbar .item-box .jsonp-box{
    position: absolute;
    left: 0;
    top: 40px;
    width: 210px;
    height: 450px;
    background-color: rgba(50, 50, 50, 0.9);
}
.navbar .item-box .jsonp-box li{
    padding: 10px 0 9px 15px;
    border-bottom: 1px solid #555;
}
.navbar .item-box .jsonp-box li.active{
    background-color: #ffffff;
}
.navbar .item-box .jsonp-box li.active .top-title a{
    color: #333333;
}
.navbar .item-box .jsonp-box li.active .tuijian a{
    color: #333333;
}
.navbar .item-box .jsonp-box li.active a:hover{
    color: #ff6600 !important;
}

.navbar .item-box .jsonp-box li .top-title{
    height: 21px;
    line-height: 21px;
}
.navbar .item-box .jsonp-box li .top-title a{
    color: #ffffff;
}
.navbar .item-box .jsonp-box li .tuijian{
    height: 22px;
    line-height: 22px;
}
.navbar .item-box .jsonp-box li .tuijian a{
    white-space: nowrap;
    color: #999;
    font-size: 12px;
    margin-right: 5px;
}
.navbar .item-box .jsonp-box li .cont-box{
    position: absolute;
    left: 210px;
    top: 0;
    width: 740px;
    padding: 10px 20px;
    background-color: white;
    min-height: 450px;
}
.navbar .item-box .jsonp-box li .cont-box .cont-box-left{
    float: left;
    width: 545px;
    padding: 0 5px;

    height: 100%;
    /* background-color: red; */
}
.navbar .item-box .jsonp-box li .cont-box .cont-box-left dl{
    color: #666666;
}
.navbar .item-box .jsonp-box li .cont-box .cont-box-left dl dt{
    font-size: 12px;
    font-weight: 700;
    color: #ff6600;
    border-bottom: 1px solid #eee;
    padding: 10px 0;
}
.navbar .item-box .jsonp-box li .cont-box .cont-box-left dl dd{
    padding-top: 5px;
    font-size: 12px;
}
.navbar .item-box .jsonp-box li .cont-box .cont-box-left dl dd a{
    float: left;
    height: 22px;
    line-height: 22px;
    white-space: nowrap;
    color: #333;
    font-size: 12px;
    margin-right: 5px;
}
.navbar .item-box .jsonp-box li .cont-box .cont-box-left dl dd a::after{
    content: "/";
    margin: 0 10px;
    color: #ddd;
}
.navbar .item-box .jsonp-box li .cont-box .cont-box-right{
    float: right;
    width: 175px;
    padding: 0 5px;

    height: 100%;
    background-color: yellow;
}
/* 分类导航区域结束 */


.navbar .navbar-mid{
    float: left;
}
.navbar .navbar-mid li{
    float: left;
    width: 56px;
    height: 40px;
    margin-left: 50px;
}
.navbar .navbar-mid a{
    display: block;
    height: 40px;
    line-height: 40px;
    font-size: 14px;
    font-weight: 700;
    color: #333333;
}
.navbar .navbar-mid a:hover{
    color: #ff6600;
}
.navbar .navbar-right{
    float: right;
    width: 215px;
    height: 35px;
    overflow: hidden;
}
.navbar .navbar-right .imgWrap1{
    float: left;
    width: 195px;
    height: 40px;
}
.navbar .navbar-right .imgWrap1 a{
    display: block;
    height: 35px;
}
.navbar .navbar-right .btns1{
    float: right;
    width: 12px;
    height: 35px;
}
.navbar .navbar-right .btns1 span{
    display: block;
    height: 15px;
    cursor: pointer;

    background-color: red;
}
.navbar .navbar-right .btns1 span.to-down{
    margin-top: 5px;
}

/* 网站导航区域结束 */




/* 网站脚注区域 */
.footer{
    /* margin-top: 0; */
}