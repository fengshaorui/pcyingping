/* 限时活动 */
.header,.bodyer,.fooder{
    width: 100%;
}
.time-limit{
    margin: 0px 0 15px 0;
    border: 1px solid #eaeaea;
    background-color: #fff;
}
.time-limit .time-limit-text{
    padding: 0 10px;
    font-size: 20px;
    height: 50px;
    line-height: 50px;
    font-weight: 700;
}
.time-limit .time-limit-text span{
    font-size: 12px;
    font-weight: 100;
    color: #666;
}
.row{
    /* margin-right: -5px; */
    width: 100%;
    margin-left: -5px;
    /* margin-bottom: 30px; */
    
}
.row .rowList{
    position: relative;
    float: left;
    margin-right: 7px;
    margin-bottom: 30px;
}
.row-detrail{
    box-sizing: border-box;
    width: 393px;
    float: left;
    /* padding-right: 5px; */
    padding-left: 5px;
    border-bottom: 1px solid #999;
    border-right: 1px solid #999;
    box-shadow: rgba(0,0,0,0.3) 2px 3px 6px;
    transition: all 1s;
}
.row-detrail .row-detrail-img{
    position: relative;
    overflow: hidden;
    height: 177px;
    
}
.row-detrail .row-detrail-img img{
    transition: all 1s;
}
.row-detrail .row-detrail-img span{
    position: absolute;
    top:0;
    left: 0;
    background: #f60;
    width: 73px;
    height: 18px;
    text-align: center;
    color: #fff;
    line-height: 18px;
}
.row-detrail .mask-layer{
    position: absolute;
    top: 0;
    left: 5px;
    width: 388px;
    height: 178px;
    opacity: 0;
    background: rgba(0, 0, 0, 0.7);
    /* display: none; */
    transition: all 1s;
}
.row-detrail .mask-layer .zhuanti{
    position: absolute;
    color: #fff;
    font-size: 18px;
    line-height: 44px;
    text-align: center;
    top: 50%;
    left: 50%;
    margin-left: -70px;
    margin-top: -22px;
    width: 140px;
    height: 44px;
    border: 1px solid #fff;
    border-radius: 2px;
}
.row-detrail h3{
    margin: 0px 10px 10px 10px;
    font-size: 16px;
    font-weight: 700;
    color: black
}
.row-detrail h3 span{
    display: inline-block;
    vertical-align: middle;
    width: 100%;
    height: 48px;
    line-height: 48px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    border-bottom: 1px solid #eee;
    transition: all 1s;
}
.row-detrail .row-desc{
    margin: 0 10px 10px 10px;
    color: #666;
    height: 60px;
    line-height: 20px;
    overflow: hidden;
}
.row-detrail .row-time{
    margin: 10px;
    color: #999;
}
.row .rowList .row-detrail a:hover .mask-layer{
    opacity: 1;
}
.row .rowList .row-detrail a:hover h3 span{
    color: #f60;
}
.row .rowList .row-detrail a:hover .row-detrail-img img{
    transform: scale(1.1,1.1);
}
.row .rowList .row-detrail:hover{
    box-shadow: rgba(0,0,0,0.3) 2px 6px 18px;
}