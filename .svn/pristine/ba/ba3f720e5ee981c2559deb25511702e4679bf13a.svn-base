/* 热卖 */

.all-body .hot-sale {
    overflow: hidden;
    margin-top: 15px;
    border: 1px solid #EDEEF0;
    margin-bottom: 15px;
}
.all-body .hot-sale .hot-sale-style{
    /* margin:15px 0; */
    width: 120px;
    height: 45px;
    float: left;
}
.all-body .hot-sale .hot-sale-style .hot-sale-recommendation{
    width: 48px;
    height: 37px;
    background: #ff6600;
    text-align: center;
    margin: 0 auto;
    position: relative;
    padding-top:8px;
}
.all-body .hot-sale .hot-sale-style .hot-sale-recommendation span{
    color:white;
    font-size: 12px;
}

.all-body .hot-sale .hot-sale-style .hot-sale-recommendation:after{
    content: '';
    border-left: 0px solid transparent;
    border-right: 40px solid transparent;
    border-top: 22px solid #ff6600;
    position: absolute;
    bottom: -22px;
    left: 0;
}
.all-body .hot-sale .hot-sale-style .hot-sale-recommendation:before{
    content: '';
    border-left: 40px solid transparent;
    border-right: 0px solid transparent;
    border-top: 22px solid #ff6600;
    position: absolute;
    bottom: -22px;
    right: 0;
}
  
.all-body .hot-sale .hot-sale-show{
    height: 140px;
    padding: 10px;
    float: left;
    overflow: hidden;
}
.all-body .hot-sale .hot-sale-show .hot-sale-list{
    width: 274px;
    height: 118px;
    float: left;
    /* background: #666; */
    padding: 10px;
    margin-right: 57px;
    border: 1px solid #EDEEF0;
}
.all-body .hot-sale .hot-sale-show .hot-sale-list a{
    float: left;
}
.all-body .hot-sale .hot-sale-show .hot-sale-list .hot-sale-list1{
    width: 50%;
    margin-top: 10px;
    padding-left: 15px;
    padding-right: 5px;
    float: left;
}
.all-body .hot-sale .hot-sale-show .hot-sale-list .hot-sale-list1 a{
    width: 145px;
    height: 35px;
    font-size: 12px;
    color: black;
    margin-bottom: 10px;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    text-overflow: ellipsis;
    overflow: hidden;
}
.all-body .hot-sale .hot-sale-show .hot-sale-list .hot-sale-list1 strong{
    color: #ff6600;
    margin-bottom: 5px;
    font-weight: normal;
    font-size: 18px;
}
.all-body .hot-sale .hot-sale-show .hot-sale-list .hot-sale-list1 .hot-sale-buy{
    margin-top: 10px;
    width: 50px;
    height: 20px;
}
.all-body .hot-sale .hot-sale-show .hot-sale-list .hot-sale-list1 .hot-sale-buy a{
    background: #ff6600;
    color:white;
    padding: 5px 8px;
    width: 50px;
    height: 20px;
}

