/* 全部商品分类 */
.banner-mid{
    display: none;
}
.top .top-mid-down .all-list:hover .banner-mid{
    display: block;
}
.banner{
    width: 1200px;
    position: relative;
    
}

.banner .banner-mid{
    position: absolute;
    top:0;
    left:50%;
    width:1200px;
    margin-left:-600px; 
}
.banner .banner-mid .banner-list{
   
    position: absolute;
    top: 0;
    left:354px;
}
.banner .banner-mid .banner-list ul{
    display: inline-block;
}
.banner .banner-mid .banner-list ul li.list{
    list-style: none;
    width:180px;
    height: 43px;
    padding:10px 15px 10px;
    background-color:rgba(0,0,0,.6);
    border-bottom: 1px solid #555555;
    color:white;
}
.banner .banner-mid .banner-list ul li.list:hover{
    background-color: white;
    color:#666666;
}
.banner .banner-mid .banner-list ul li.last{
    height: 45px;
}
.banner .banner-mid .banner-list .list-item{
    font-size: 12px;
    margin-top:4px;
    color:#999999;
}
.banner .banner-mid .banner-list .list-hid{
    position: absolute;
    top:0;
    left:210px;
    width: 770px;
    background-color:white;
    color:black;
    min-height: 470px;
    padding:10px 20px;
    display: none;
    box-shadow: 8px -5px 5px -5px #666666;
}
.banner .banner-mid .banner-list .list-hid .left{
    float:left;
}
.banner .banner-mid .banner-list .list-hid .left p{
    width:550px;
    height:30px; 
    line-height: 30px;
    color:#FF6600;
    border-bottom:1px solid #EDEEF0;
}
.banner .banner-mid .banner-list .list-hid div.left{
    width:550px;
    color:#999999;
    font-size: 12px;
    margin-top:5px;
}
.banner .banner-mid .banner-list .list-hid div.left span{
    margin-right:10px;
    height: 25px;
    line-height: 25px;
}
.banner .banner-mid .banner-list .list-hid div.left a:link{
    color:#999999;
}
.banner .banner-mid .banner-list .list-hid div.left a:hover{
    color:#FF6600;
}
.banner .banner-mid .banner-list .list-hid div.left a:visited{
    color:#999999;
}
.banner .banner-mid .banner-list .list-hid div.left a:active{
    color:#999999;
}
.banner .banner-mid .banner-list .list-hid .right{
    float:left;
    width: 170px;
    margin-left:20px;
    margin-top:10px;
}
.banner .banner-mid .banner-list .list-hid .right .right-top p,.right-bottom p{
    color:#FF6600;
    padding-bottom:5px;
    border-bottom:1px solid #EDEEF0;
} 
.banner .banner-mid .banner-list .list-hid .right .right-top span,.right-bottom span{
    display: inline-block;
    width: 80px;
    height: 30px;
    background-color: #FF6600;
    color:white;
    margin-top:8px;
    text-align: center;
    line-height: 30px;
    font-size: 12px;
}
.banner .banner-mid .banner-list .list-hid .right p{
    margin-top:10px;
}
.banner .banner-mid .banner-list .list-hid .right .right-bottom span img{
    width:80px;
}
/* 全部商品分类结束 */
