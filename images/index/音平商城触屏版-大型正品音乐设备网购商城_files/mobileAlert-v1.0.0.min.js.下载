function showMobileAlert(e){var o=$("#mobileShowMessageDiv");$("#mobileShowMessageWarning").text(e),$(o).show(300),setTimeout(function(){$(o).hide(300)},1500)}function showMobileAlertAndCallBack(e,o){var i=$("#mobileShowMessageDiv");$("#mobileShowMessageWarning").text(e),$(i).show(300),setTimeout(function(){$(i).hide(300),"function"==typeof showMobileAlertCallBack&&1==parseInt(o)&&showMobileAlertCallBack()},1500)}